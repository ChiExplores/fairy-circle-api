    "express": "~4.16.1",
    "express-graphql": "^0.11.0",
    "graphql": "^14.7.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.0",
    "knex": "^0.21.1",
    "morgan": "~1.9.1",
    "pg": "^8.2.1"
  },
  "devDependencies": {
    "mocha": "^8.0.1"
  }
}
